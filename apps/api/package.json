{
  "name": "@buzztrip/api",
  "version": "0.0.0",
  "main": "src/index.ts",
  "scripts": {
    "clean:all": "rm -rf node_modules/",
    "dev": "wrangler types && wrangler dev src/index.ts",
    "dev:custom-cert": "--https-cert-path certificates/localhost+3.pem --https-key-path certificates/localhost+3-key.pem --local-protocol=https",
    "deploy": "wrangler types && wrangler deploy --minify src/index.ts",
    "check:types": "tsc --noEmit",
    "cf-typegen": "wrangler types"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.787.0",
    "@buzztrip/db": "workspace:*",
    "@buzztrip/transactional": "workspace:*",
    "@hono/sentry": "^1.2.0",
    "@hono/swagger-ui": "^0.5.0",
    "@hono/zod-openapi": "^0.18.4",
    "@hono/zod-validator": "^0.4.3",
    "better-auth": "^1.2.4",
    "file-type": "^20.4.1",
    "hono": "^4.7.1"
  },
  "devDependencies": {
    "wrangler": "^4.14.1"
  }
}
