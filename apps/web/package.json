{
  "name": "@buzztrip/web",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "cf-typegen": "wrangler types --env-interface CloudflareEnv env.d.ts",
    "pages:build": "bunx @cloudflare/next-on-pages",
    "preview": "bun pages:build && wrangler pages dev",
    "deploy": "bun pages:build && wrangler pages deploy",
    "studio:dev": "drizzle-kit studio",
    "studio:prd": "cross-env NODE_ENV=production drizzle-kit studio",
    "db:migrate": "drizzle-kit migrate",
    "migrate:prd": "bunx wrangler d1 migrations apply BuzzTrip-db --remote",
    "migrate:dev": "bunx wrangler d1 migrations apply BuzzTrip-db --local"
  },
  "dependencies": {
