    "@uidotdev/usehooks": "^2.4.1",
    "@vis.gl/react-google-maps": "^1.1.1",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "cmdk": "^1.0.0",
    "drizzle-orm": "^0.33.0",
    "framer-motion": "^11.4.0",
    "lucide-react": "^0.438.0",
    "next": "^14.2.7",
    "next-axiom": "^1.4.1",
